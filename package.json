{
  "name": "httpbook-grid",
  "displayName": "httpBook - Grid Renderer",
  "description": "httpbook extension to view data in grid",
  "version": "0.7.0",
  "publisher": "anweber",
  "homepage": "https://github.com/AnWeber/httpbook-grid",
  "repository": {
    "type": "git",
    "url": "https://github.com/AnWeber/httpbook-grid"
  },
  "bugs": {
    "url": "https://github.com/AnWeber/httpbook-grid/issues"
  },
  "qna": "https://github.com/AnWeber/httpbook-grid/discussions",
  "engines": {
    "vscode": "^1.57.0"
  },
  "categories": [
    "Notebooks"
  ],
  "keywords": [
    "httpbook",
    "rest client",
    "http"
  ],
  "icon": "icon.png",
  "preview": true,
  "enableProposedApi": true,
  "activationEvents": [
    "onNotebook:http",
    "onLanguage:http"
  ],
  "main": "./dist/extension.js",
  "capabilities": {
    "virtualWorkspaces": true
  },
  "contributes": {
    "notebookRenderer": [
      {
        "id": "httpbook-grid",
        "entrypoint": "./dist/gridRenderer.js",
        "displayName": "httpbook-grid",
        "mimeTypes": [
          "x-application/httpbook-grid"
        ]
      }
    ],
    "configuration": {
      "type": "object",
      "title": "httpbook-grid",
      "properties": {
        "httpbook-grid.numberOfRowsForColDefRecognition": {
          "type": "number",
          "default": 10,
          "scope": "window",
          "description": "number of rows for automatic coldef recognition"
        },
        "httpbook-grid.searchBodyForArray": {
          "type": "boolean",
          "default": true,
          "scope": "window",
          "description": "search for array in response"
        },
        "httpbook-grid.gridOptions": {
          "type": "object",
          "default": {
            "enableBrowserTooltips": true,
            "enableCellTextSelection": true,
            "defaultColDef": {
              "resizable": true,
              "filter": true,
              "sortable": true
            }
          },
          "scope": "window",
          "description": "default ag-grid gridOptions"
        }
      }
    }
  },
  "extensionDependencies": [
    "anweber.httpbook"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "webpack --mode production --config ./webpack.config.js",
    "watch": "webpack --mode development --watch --config ./webpack.config.js",
    "lockfile-lint": "lockfile-lint --path package-lock.json --validate-https --allowed-hosts npm --validate-checksum --validate-integrity",
    "eslint": "eslint src --ext ts",
    "lint": "npm run eslint && npm run lockfile-lint",
    "precommit": "npm run lint"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.174",
    "@types/node": "^16.10.1",
    "@types/react": "^17.0.24",
    "@types/vscode": "^1.60.0",
    "@types/vscode-notebook-renderer": "^1.60.0",
    "@types/webpack-env": "^1.16.2",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "css-loader": "^6.3.0",
    "eslint": "^7.32.0",
    "eslint-plugin-node": "^11.1.0",
    "fork-ts-checker-webpack-plugin": "^6.3.3",
		"httpyac": "^4.0.4",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "lockfile-lint": "^4.6.2",
    "style-loader": "^3.3.0",
    "thread-loader": "^3.0.4",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.3",
    "url-loader": "^4.1.1",
    "vscode-dts": "^0.3.1",
    "webpack": "^5.54.0",
    "webpack-cli": "^4.8.0"
  },
  "dependencies": {
    "@ag-grid-community/client-side-row-model": "^26.0.0",
    "@ag-grid-community/core": "^26.2.0",
    "lodash": "^4.17.21",
    "preact": "^10.5.14",
    "vscode-notebook-error-overlay": "^1.0.1"
  }
}
